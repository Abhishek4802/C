#include <stdio.h>

int main() {
    int arr[5] = {1, 2, 3, 4, 5};

    // accessing array elements
    printf("Array elements: ");
    for (int i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    // changing array elements
    arr[2] = 10;
    printf("Array elements after changing arr[2] to 10: ");
    for (int i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    // array size and memory allocation
    int size = sizeof(arr) / sizeof(arr[0]);
    printf("Array size: %d\n", size);

    // multidimensional arrays
    int matrix[2][3] = {{1, 2, 3}, {4, 5, 6}};
    printf("Matrix elements:\n");
    for (int i = 0; i < 2; i++) {
        for (int j = 0; j < 3; j++) {
            printf("%d ", matrix[i][j]);
        }
        printf("\n");
    }

    return 0;
}
